-- Scripts:

local function EKJFY_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		workspace.muffy.Head:Destroy()
	end)
end
coroutine.wrap(EKJFY_fake_script)()
local function XHSWOXJ_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		local c = game.Players.LocalPlayer.Character
		local victim = script.Parent.Parent.TextBox.Text
		c.HumanoidRootPart.CFrame = workspace.muffy.Torso.CFrame
		wait(0.1)
		workspace.muffy.Torso.CFrame = workspace:FindFirstChild(victim).Head.CFrame
		c.HumanoidRootPart.CFrame = workspace:FindFirstChild(victim).Head.CFrame
		wait(.1)
		workspace.muffy.Head:Destroy()
	end)
end
coroutine.wrap(XHSWOXJ_fake_script)()
local function IKMLHQE_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	script.Parent.MouseButton1Click:Connect(function()
		local c = game.Players.LocalPlayer.Character
		c.HumanoidRootPart.CFrame = workspace.muffy.Torso.CFrame
		for i,v in pairs(c:GetChildren()) do
			if v:IsA("BasePart") then
				v.Touched:Connect(function(a) 
					if a.Parent.Name == "muffy" then
						if isnetworkowner(a.Parent.Torso) then
							a.Parent.Torso.CFrame = c.RightHand.CFrame
						end
					end
				end)
			end
		end
	end)
end
coroutine.wrap(IKMLHQE_fake_script)()
local function QFFNPMM_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	script.Parent.MouseButton1Click:Connect(function()
		local c = game.Players.LocalPlayer.Character
		c.HumanoidRootPart.CFrame = workspace.muffy.Torso.CFrame
		for i,v in pairs(c:GetChildren()) do
			if v:IsA("BasePart") then
				v.Touched:Connect(function(a) 
					if a.Parent.Name == "muffy" then
						if isnetworkowner(a.Parent.Torso) then
							a.Parent.Torso.CFrame = a.Parent.Torso.CFrame * CFrame.Angles(0,10,0)
						end
					end
				end)
			end
		end
	end)
end
coroutine.wrap(QFFNPMM_fake_script)()
local function WZSQI_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = gui:TweenPosition(UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y), 'Out', 'Linear', 0, true); -- drag speed
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(WZSQI_fake_script)()
local function UYKAII_fake_script() -- TextButton_5.LocalScript 
	local script = Instance.new('LocalScript', TextButton_5)

	script.Parent.MouseButton1Click:Connect(function()
		for i,v in pairs(workspace.muffy.Head:GetChildren()) do
			if v:IsA("Sound") then
				v:Play()
			end
		end
	end)
end
coroutine.wrap(UYKAII_fake_script)()
